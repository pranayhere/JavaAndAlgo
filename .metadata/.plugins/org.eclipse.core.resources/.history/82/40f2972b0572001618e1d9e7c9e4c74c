package com.pranay.GeeksForGeeks;

public class STKReverseString {
	public static Stack stk = new Stack(10);
	
	public String reverse(char arr[]) {
		for(int i=0; i<arr.length; i++) {
			stk.push(arr[i]);
		}
		String str1 = "";
		for(int i=0; i<arr.length; i++) {
			str1 = str1.concat(""+stk.pop());
		}
		return str1;
	}
}
